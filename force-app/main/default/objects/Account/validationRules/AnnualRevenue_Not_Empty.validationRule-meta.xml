<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AnnualRevenue_Not_Empty</fullName>
    <active>false</active>
    <errorConditionFormula>OR (   ISBLANK(AnnualRevenue ) , AnnualRevenue &lt; 1000 )</errorConditionFormula>
    <errorDisplayField>AnnualRevenue</errorDisplayField>
    <errorMessage>Annual Revenue should be more than 1000</errorMessage>
</ValidationRule>
